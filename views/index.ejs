<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sjita Car-Wash</title>
    <link rel="stylesheet" href="css/main.css">
    
  
</head>
<body>

    <!-- Navbar Section -->
    <nav class="navbar">
        <div class="navbar__container">
            <a href="/" id="navbar__logo"><h1>Nine&copy</h1></a>
            <div class="navbar__toggle" id="mobile-menu">
                <span class="bar"></span>
                <span class="bar"></span>
                <span class="bar"></span>
            </div>
            <ul class="navbar__menu">
                <li class="navbar__item">
                    <a href="#" class="navbar__links">Home</a>
                </li>
                <li class="navbar__item">
                    <a href="#" class="navbar__links">About</a>
                </li>
                <li class="navbar__item">
                    <a href="#" class="navbar__links">Contact</a>
                </li>
                <li class="navbar__btn">
                    <a href="/login" class="button">Sign Up</a>
                </li>
            </ul>
        </div>
    </nav>




<!-- Hero Section -->
<div class="main">
    <div class="main__container">
        <div class="main__content">
            <h1><span class="text-highlight">WE</span> Not Like You. <span class="text-highlight">BE</span> Like Us.</h1>
            <p>Discover our <span>Work</span> Below</p>
            <button class="main__btn"><a href="/signup">View Our Work</a></button>
        </div>
        <div class="main__img--container">
            <img src="img/sjita_logo.png" id="main__img" style="border-radius:30%;">
        </div>
    </div>
</div>

<!-- Images Section -->
<div class="images">
    <h1>Experience The Unique <span>Car-Wash</span></h1>
    <div class="images__container">
        <div class="images__card">
            <img src="img/loge1.png" width="400px" height="425px" style="border-radius:15%;">
            <h2>sjita spear</h2>
            <p>Ape #0001</p>
        </div>
        <div class="images__card">
            <img src="img/logo2.png"  width="400px" height="425px" style="border-radius:15%;">
            <h2>Sjita coins</h2>
            <p>Ape #0002</p>
        </div>
        <div class="images__card">
            <img src="img/logo8.png"  width="400px" height="425px" style="border-radius:15%;">
            <h2>Sjita Bidder</h2>
            <p>Ape #0003</p>
        </div>
        <div class="images__card">
            <img src="img/logo4.png"  width="400px" height="425px" style="border-radius:15%;">
            <h2>Sjita Crypto cats</h2>
            <p>Ape #0024</p>
        </div>
        <div class="images__card">
            <img src="img/logo5.png"  width="400" height="425px" style="border-radius:15%;">
            <h2>Corner Crypto </h2>
            <p>Ape #0169</p>
        </div>
        <div class="images__card">
            <img src="img/logo6.png"  width="400px" height="425px" style="border-radius:15%;">
            <h2>Sjita Crypto</h2>
            <p>Ape #7208</p>
        </div>
    </div>

</div>

<!-- Footer Section -->
<div class="footer__container">
    <p class="website__rights">Sjita Car-Wash |  2022. All rights reserved</p>
</div>

<script>
    const menu = document.querySelector('#mobile-menu');
    const menuLinks = document.querySelector('.navbar__menu');


    menu.addEventListener('click', function() {
    menu.classList.toggle('is-active')
    menuLinks.classList.toggle('active')
    });

</script>

</body>
</html>



